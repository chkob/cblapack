(require 'cl)

(defun replace-in-files (matches files)
  (save-excursion
    (mapc (lambda (file)
	    (switch-to-buffer (or (find-buffer-visiting file) (find-file file)))
	    (mapc (lambda (x)
		    (beginning-of-buffer)
		    (let ((case-fold-search nil))
		      (print x)
		      (while (search-forward-regexp (car x) nil t)
			(replace-match (cdr x) nil t)))
		    (save-buffer)
		    )
		  matches))
	  files)))

(defvar cb-search-string)

(defun query-replace-cb (from-string to-string &optional delimited start end)
  (interactive (query-replace-read-args "Query replace" nil))
  (setq cb-search-string from-string)
  (let ((remaining (member (buffer-file-name (current-buffer)) cb-files)))
    (dolist (i (or remaining cb-files))
      (let ((b (find-buffer-visiting i)))
	(unless (equal b (current-buffer))
	  (switch-to-buffer b)
	  (beginning-of-buffer)))
      (perform-replace from-string to-string t nil delimited nil nil
		       start end))))

(defun query-replace-regexp-cb (from-string to-string &optional delimited start end)
  (interactive (query-replace-read-args "Query replace" nil))
  (setq cb-search-string from-string)
  (let ((remaining (member (buffer-file-name (current-buffer)) cb-files)))
    (dolist (i (or remaining cb-files))
      (let ((b (find-buffer-visiting i)))
	(unless (equal b (current-buffer))
	  (switch-to-buffer b)
	  (beginning-of-buffer)))
      (query-replace-regexp from-string to-string delimited start end))))

(defun search-cb (string)
  (interactive "sString: ")
  (setq cb-search-string string)
  (let ((remaining (member (buffer-file-name (current-buffer)) cb-files)))
    (dolist (i (or remaining cb-files))
      (let ((b (find-buffer-visiting i)))
	(unless (equal b (current-buffer))
	  (print b)
	  (switch-to-buffer b)
	  (beginning-of-buffer)))
      (print '*)
      (setq case-fold-search t)
      (if (search-forward string nil t)
	  (return)))))

(defun search-next-cb ()
  (interactive)
  (search-cb cb-search-string))

(defun back-to-emacs ()
  (interactive)
  (switch-to-buffer "emacs.el"))

(defun next-cb ()
  (interactive)
  (let ((remaining (member (buffer-file-name (current-buffer)) cb-files)))
    (when (cdr remaining)
      (switch-to-buffer (find-buffer-visiting (cadr remaining))))))

(global-set-key [?\M-p ?\C-i] 'back-to-emacs)
(global-set-key [?\M-p ?\C-s] 'search-cb)
(global-set-key [?\M-p ?\C-n] 'search-next-cb)
(global-set-key [?\M-p ?\C-m] 'next-cb)
(global-set-key [?\M-p ?\C-p] 'cb-load-symbols)

(setq auto-mode-alist (acons "\\.d\\'" 'c-mode auto-mode-alist))

